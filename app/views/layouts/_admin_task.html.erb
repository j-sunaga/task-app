    <tbody>
      <% @tasks.each do |task| %>
        <tr>
          <td class="task_row"><%= task.name %></td>
          <td><%= task.detail %></td>
          <td><%= task.deadline %></td>
          <td><%= task.status_i18n %></td>
          <td><%= task.priority_i18n %></td>
        </tr>
      <% end %>
    </tbody>
